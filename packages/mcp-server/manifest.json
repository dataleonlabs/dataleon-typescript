{
  "dxt_version": "0.1",
  "name": "@dataleon/dataleon-mcp",
  "version": "0.1.0-alpha.13",
  "description": "The official MCP Server for the Dataleon API",
  "author": {
    "name": "Dataleon",
    "email": "support@dataleon.ai"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dataleonlabs/dataleon-typescript.git"
  },
  "homepage": "https://github.com/dataleonlabs/dataleon-typescript/tree/main/packages/mcp-server#readme",
  "documentation": "https://docs.dataleon.ai",
  "server": {
    "type": "node",
    "entry_point": "${__dirname}/index.js",
    "mcp_config": {
      "command": "node",
      "args": ["${__dirname}/index.js"],
      "env": {
        "DATALEON_API_KEY": "${user_config.DATALEON_API_KEY}"
      }
    }
  },
  "user_config": {
    "DATALEON_API_KEY": {
      "title": "api_key",
      "description": "API key needed to authorize requests.  You must provide a valid API key in the `Api-Key` header. Get your API key from the Dataleon dashboard.\n",
      "required": true,
      "type": "string"
    }
  },
  "tools_generated": true,
  "compatibility": {
    "runtimes": {
      "node": ">=18.0.0"
    }
  },
  "keywords": ["api"]
}
